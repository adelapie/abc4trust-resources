    <att:Attribute>
		<att:ID>Age</att:ID>
		<att:Description xml:lang="DE">
			Alter.
			Optional kann eine Zeitzone (Offset) im ISO 8601 Format (+-hh[[:]mm]) oder als TZ Name angegeben werden.
		</att:Description>
		<att:Description xml:lang="EN">
			Age. Optionally a time zone offset in ISO 8601 format (+-hh[[:]mm]) or a TZ name can be given.
		</att:Description>
		<att:Example>request: +01, response: 42</att:Example>
		<att:Example>request: +01:00, response: 42</att:Example>
		<att:Example>request: Europe/Berlin, response: 42</att:Example>
		<att:InputFormat>
			{http://www.w3.org/2001/XMLSchema}normalizedString
		</att:InputFormat>
		<att:OutputFormat>
			{http://www.w3.org/2001/XMLSchema}nonNegativeInteger
		</att:OutputFormat>
		<att:URI>http://www.id4health.de/att/Age</att:URI>
		<att:URI>http://www.skidentity.de/att/Age</att:URI>
		<att:URI>http://www.futureid.org/att/Age</att:URI>
		<att:FriendlyName xml:lang="DE">Alter</att:FriendlyName>
		<att:FriendlyName xml:lang="EN">Age</att:FriendlyName>
    </att:Attribute>


