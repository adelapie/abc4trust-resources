    <att:Attribute>
		<att:ID>Profile-URI</att:ID>
		<att:Description xml:lang="DE">
			Benutzerprofil URI.
			Input ist das Präfix der angefragten URI. Z.B. https://www.facebook.com
		</att:Description>
		<att:Description xml:lang="EN">
			User profile URI.
			Input is the prefix of the requested URI. E.g. https://www.facebook.com
		</att:Description>
		<att:Example>https://www.facebook.com/florianvk</att:Example>
		<att:Example>http://www.linkedin.com/pub/open-ecard/63/443/a99</att:Example>
		<att:Example>https://www.xing.com/profile/Detlef_Huehnlein</att:Example>
		<att:InputFormat>
			{http://www.w3.org/2001/XMLSchema}anyURI
		</att:InputFormat>
		<att:OutputFormat>
			{http://www.w3.org/2001/XMLSchema}anyURI
		</att:OutputFormat>
		<att:URI>http://www.id4health.de/att/profile-uri</att:URI>
		<att:URI>http://www.skidentity.de/att/profile-uri</att:URI>
		<att:URI>http://www.futureid.org/att/profile-uri</att:URI>
		<att:FriendlyName xml:lang="DE">Profil-URI</att:FriendlyName>
		<att:FriendlyName xml:lang="EN">Profile-URI</att:FriendlyName>
    </att:Attribute>


