    <!-- =========================== -->
    <!-- Common Personal Attributes  -->
    <!-- =========================== -->

    <att:Attribute>
		<att:ID>CommonName</att:ID>
		<att:Description xml:lang="DE">
			Spitzname, ID in einem sozialen Netz oder CommonName in einem X.509 Zertifikat
		</att:Description>
		<att:Description xml:lang="EN">
			Subject's alias, ID in a social network or CommonName in an X.509 certificate
		</att:Description>
		<att:Example>Feli3</att:Example>
		<att:OutputFormat>
			{http://www.w3.org/2001/XMLSchema}normalizedString
		</att:OutputFormat>
		<att:URI>http://axschema.org/namePerson/friendly</att:URI>
		<att:URI>http://www.id4health.de/att/CommonName</att:URI>
		<att:URI>http://www.skidentity.de/att/CommonName</att:URI>
		<att:URI>http://www.futureid.org/att/CommonName</att:URI>
		<att:FriendlyName xml:lang="DE">Common Name</att:FriendlyName>
		<att:FriendlyName xml:lang="EN">Common Name</att:FriendlyName>
    </att:Attribute>

    <att:Attribute>
		<att:ID>FirstName</att:ID>
