    <!-- ================================= -->
    <!-- Communication Related Attributes  -->
    <!-- ================================= -->

    <att:Attribute>
		<att:ID>Email</att:ID>
		<att:Description xml:lang="DE">
			E-Mail Adresse
		</att:Description>
		<att:Description xml:lang="EN">
			Internet SMTP email address as per RFC2822
		</att:Description>
		<att:Example>john@isp.example.com</att:Example>
		<att:OutputFormat>
			{http://www.w3.org/2001/XMLSchema}normalizedString
		</att:OutputFormat>
		<att:URI>http://axschema.org/contact/email</att:URI>
		<att:URI>http://www.id4health.de/att/email</att:URI>
		<att:URI>http://www.skidentity.de/att/email</att:URI>
		<att:URI>http://www.futureid.org/att/email</att:URI>
		<att:FriendlyName xml:lang="DE">E-Mail</att:FriendlyName>
		<att:FriendlyName xml:lang="EN">E-Mail</att:FriendlyName>
    </att:Attribute>

    <att:Attribute>
		<att:ID>Phone</att:ID>
		<att:Description xml:lang="DE">
			Telefonnummern in Form von tel: (RFC 3966) oder sip:/ sips: URIs (RFC 3261).
			Input ist optional und kann verwendet werden um nur bestimmte Nummertypen (siehe Type Element) anzufragen.
		</att:Description>
		<att:Description xml:lang="EN">
			Phone numbers formatted as tel: (RFC 3966) or sip:/ sips: URIs (RFC 3261).
			Input is optional and can be used to request only specific number types (see Type element).
		</att:Description>
		<att:Example>
			response:
			<PhoneNumber>
				<Value>tel:+49-89-123-123-123</Value>
			</PhoneNumber>
			<PhoneNumber>
				<Type>mobile</Type>
				<Value>tel:+49-171-123-123-123</Value>
			</PhoneNumber>
			<PhoneNumber>
				<Type>voip</Type>
				<Value>sips:user@example.com</Value>
			</PhoneNumber>
		</att:Example>
		<att:InputFormat>
			{http://www.w3.org/2001/XMLSchema}normalizedString
		</att:InputFormat>
		<att:OutputFormat>
			{http://www.skidentity.de/att}PhoneNumberType
		</att:OutputFormat>
		<att:URI>http://axschema.org/contact/phone</att:URI>
		<att:URI>http://www.id4health.de/att/phone</att:URI>
		<att:URI>http://www.skidentity.de/att/phone</att:URI>
		<att:URI>http://www.futureid.org/att/phone</att:URI>
		<att:FriendlyName xml:lang="DE">Telefonnummer</att:FriendlyName>
		<att:FriendlyName xml:lang="EN">Phone</att:FriendlyName>
    </att:Attribute>


