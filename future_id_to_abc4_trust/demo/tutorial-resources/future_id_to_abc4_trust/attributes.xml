    <att:Attribute>
		<att:ID>eIdentifier</att:ID>
		<att:Description xml:lang="DE">
			Sektorspezifischer Identifikator (Pseudonym)
		</att:Description>
		<att:Description xml:lang="EN">
			Sector specific identifier
		</att:Description>
		<att:Example>A1124711AB14</att:Example>
		<att:OutputFormat>
			{http://www.w3.org/2001/XMLSchema}normalizedString
		</att:OutputFormat>
		<att:URI>http://www.id4health.de/att/eIdentifier</att:URI>
		<att:URI>http://www.skidentity.de/att/eIdentifier</att:URI>
		<att:URI>http://www.futureid.org/att/eIdentitfier</att:URI>
		<att:FriendlyName xml:lang="DE">Pseudonym</att:FriendlyName>
		<att:FriendlyName xml:lang="EN">eIdentifier</att:FriendlyName>
    </att:Attribute>

    <att:Attribute>
		<att:ID>AgeVerification</att:ID>
		<att:Description xml:lang="DE">
			Altersverifikation. Siehe auch Attribut Age.
		</att:Description>
		<att:Description xml:lang="EN">
			Age verification. See also attribute Age.
		</att:Description>
		<att:Example>
			request:
			<saml:AttributeValue xmlns:saml="urn:oasis:names:tc:SAML:2.0:assertion" xmlns="http://www.skidentity.de/att">
				<ReferenceValue>18</ReferenceValue>
				<TimeZone>Europe/Berlin</TimeZone>
			</saml:AttributeValue>
			response: true
		</att:Example>
		<att:InputFormat>
			{http://www.skidentity.de/att}AgeVerificationInputType
		</att:InputFormat>
		<att:OutputFormat>
			{http://www.w3.org/2001/XMLSchema}boolean
		</att:OutputFormat>
		<att:URI>http://www.id4health.de/att/AgeVerification</att:URI>
		<att:URI>http://www.skidentity.de/att/AgeVerification</att:URI>
		<att:URI>http://www.futureid.org/att/AgeVerification</att:URI>
		<att:FriendlyName xml:lang="DE">Altersverifikation</att:FriendlyName>
		<att:FriendlyName xml:lang="EN">Age verification</att:FriendlyName>
    </att:Attribute>

